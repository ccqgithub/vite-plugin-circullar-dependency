{
  "name": "vite-plugin-circullar-dependency",
  "version": "0.0.1",
  "description": "Check circular dependency in vite project",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccqgithub/vite-plugin-circullar-dependency.git"
  },
  "author": "Season Chen",
  "license": "MIT",
  "scripts": {
    "dev": "node build.js --watch",
    "dev:es": "vite build --watch",
    "dev:cjs": "tsc --watch",
    "build": "node build.js",
    "build:es": "vite build",
    "build:cjs": "tsc",
    "test": "vite -c test/vite.config.ts",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "devDependencies": {
    "@types/node": "^17.0.33",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "vite": "*",
    "rollup": "*"
  },
  "keywords": [
    "vite",
    "plugin",
    "circular",
    "dependency"
  ],
  "files": [
    "dist",
    "src"
  ]
}